    "playwright": "^1.55.0",
    "postcss": "8.4.47",
    "tailwindcss": "3.4.13",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "5.5.4",
    "wait-on": "^8.0.4"
  }
}
