{
  "name": "summit-chronicles",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "e2e": "playwright test",
    "prepare": "husky install",
    "index:rag": "node scripts/index-rag.cjs",
    "index:rag:db": "node -r dotenv/config scripts/index-rag.cjs dotenv_config_path=.env.local",
    "release:preflight": "node scripts/release-check.cjs",
    "print:env": "dotenv -e .env.local -- node scripts/print-env.mjs",
    "deploy": "netlify deploy --prod",
    "ingest:local": "node -r dotenv/config scripts/ingest.mjs dotenv_config_path=.env.local",
    "ingest:md:local": "INGEST_ENDPOINT=http://localhost:3000/api/db/ingest node -r dotenv/config scripts/ingest-md.mjs content dotenv_config_path=.env.local",
    "ingest:md:prod": "INGEST_ENDPOINT=https://heroic-figolla-ba583d.netlify.app/api/db/ingest node -r dotenv/config scripts/ingest-md.mjs content dotenv_config_path=.env.local",
    "curl:ingest": "dotenv -e .env.local -- sh -c 'curl -s -X POST http://localhost:3000/api/db/ingest -H \"content-type: application/json\" -H \"x-ingest-secret: $INGEST_SECRET\" -d \"{ \\\"title\\\": \\\"expeditions\\\", \\\"source\\\": \\\"page/expeditions\\\", \\\"url\\\": \\\"/expeditions\\\", \\\"access\\\": \\\"public\\\", \\\"text\\\": \\\"The next expedition in the Summit Chronicles is Everest (target 2027). Training includes long carries, altitude work, and resilience systems.\\\" }\" | jq'",
    "test:e2e": "playwright test --reporter=html && playwright show-report",
    "design-review": "ts-node scripts/run-design-review.ts",
    "design-review:localhost": "ts-node scripts/run-design-review.ts http://localhost:3000",
    "design-review:prod": "TEST_PRODUCTION=true ts-node scripts/run-design-review.ts https://heroic-figolla-ba583d.netlify.app",
    "test:design": "playwright test e2e/design-review.spec.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@sentry/nextjs": "^10.11.0",
    "@supabase/supabase-js": "^2.57.2",
    "@tiptap/extension-hard-break": "^3.4.1",
    "@tiptap/extension-highlight": "^3.4.1",
    "@tiptap/extension-image": "^3.4.1",
    "@tiptap/extension-link": "^3.4.1",
    "@tiptap/extension-placeholder": "^3.4.1",
    "@tiptap/extension-text-align": "^3.4.1",
    "@tiptap/extension-underline": "^3.4.1",
    "@tiptap/pm": "^3.4.1",
    "@tiptap/react": "^3.4.1",
    "@tiptap/starter-kit": "^3.4.1",
    "@types/uuid": "^10.0.0",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "d3-geo": "^3.1.1",
    "d3-scale": "^4.0.2",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "fast-glob": "^3.3.2",
    "framer-motion": "^10.18.0",
    "gray-matter": "^4.0.3",
    "leaflet": "^1.9.4",
    "lottie-react": "^2.4.1",
    "lowlight": "^3.3.0",
    "lucide-react": "^0.452.0",
    "next": "14.2.13",
    "next-auth": "^4.24.11",
    "next-seo": "^6.5.0",
    "novel": "^1.0.2",
    "openai": "^4.104.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-intersection-observer": "^9.16.0",
    "react-leaflet": "^4.2.1",
    "react-markdown": "^10.1.0",
    "recharts": "^3.1.2",
    "remark-gfm": "^4.0.1",
    "socket.io-client": "^4.8.1",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.1.0",
    "web-vitals": "^5.1.0",
    "xlsx": "^0.18.5",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.2",
